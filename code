class Shape:
  def _init_(self,color,is_filled):
    self.color = color
    self.is_filled = is_filled


class Circle(Shape):
 def _init_(self, color, is_filled, radius):
    super()._init_(color, is_filled)
    self.radius = radius
   

class Square(Shape):
    def _init_(self, color, is_filled, width):
     super()._init_(color, is_filled)
     self.width = width 

class Triangle(Shape):
    def _init_(self,color, is_filled, width, height):
     super()._init_(color, is_filled)
     self.width = width
     self.height = height

circle= Circle(color="red", is_filled=True,radius=5) 
square= Square(color="blue",is_filled=False,width=6)
triangle= Triangle(color="yellow", is_filled=True,width=7)

print(triangle.color)
print(triangle.is_filled)
print(f"{triangle.width}cm")
print(f"{triangle.height}cm")
